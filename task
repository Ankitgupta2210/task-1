n=int(input("Enter the nomber :"))
f=1
while (n>0):
    f=f*n
    n=n-1

    
print(f)    


n=int(input("enter the nomber :"))
f=1
for i in range(1,n+1):
    f=f*i
    
    
print(f) 




print("python")

a=int(input("enter the nomber :"))
b=int(input("Enter the nomber :"))
c=int(input("enter the nomber :"))
maximum=a
i=2
while i>0:
        if b> maximum:
           maximum = b
        if  c > maximum:
            maximum = c
        i -= 1
    
print('the maximum number is ',maximum)    



numbers = []
n = int(input("Enter the number of numbers: "))

greatest = numbers[0]

for num in numbers:
    if num > greatest:
        greatest = num

print(f"The greatest number is:Â {greatest}")